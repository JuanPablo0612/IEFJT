<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:name=".IEFJTApp"
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.IEFJT"
        tools:targetApi="31">
        <activity
            android:name=".ui.MainActivity"
            android:exported="true"
            android:theme="@style/Theme.IEFJT">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".ui.inventory.brands.BrandSelectorActivity"
            android:exported="false"
            android:theme="@style/Theme.IEFJT" />

        <activity
            android:name=".ui.inventory.headquarters.HeadquartersSelectorActivity"
            android:exported="false"
            android:theme="@style/Theme.IEFJT" />

        <activity
            android:name=".ui.inventory.statuses.StatusSelectorActivity"
            android:exported="false"
            android:theme="@style/Theme.IEFJT" />

        <activity
            android:name=".ui.inventory.type.TypeSelectorActivity"
            android:exported="false"
            android:theme="@style/Theme.IEFJT" />
    </application>

</manifest>